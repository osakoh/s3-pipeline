{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:PutObjectVersionAcl",
        "s3:ListBucket",
        "s3:GetBucketLocation",
        "s3:GetObjectVersion"
      ],
      "Resource": [
        "arn:aws:s3:::001-staging-website-bucket/*",
        "arn:aws:s3:::001-prod-website-bucket/*" ,
        "arn:aws:s3:::001-staging-website-bucket",
        "arn:aws:s3:::001-prod-website-bucket"
      ]
    }
  ]
}
